name: Release books

on: [srelease]

jobs:
  build:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout
        uses: actions/checkout@v1
      - name: Run Pandoc Book (Docker)
        uses: docker://merovex/pandoc-book:latest
        env:
          GITHUB_TOKEN: ${{ secrets.GITHUB_TOKEN }}
test
